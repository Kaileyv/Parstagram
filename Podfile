  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Parstagram
   pod 'Parse'
  target 'ParstagramTests' do
    inherit! :search_paths
    # Pods for testing
   pod 'Parse'
  end

  target 'ParstagramUITests' do
    # Pods for testing
   pod 'Parse'
  end

